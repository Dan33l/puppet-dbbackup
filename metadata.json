{
  "name": "puppet-dbbackup",
  "version": "0.1.0",
  "summary": "Install a generic database backup solution for mysql and postgres",
  "author": "Vox Pupuli",
  "dependencies": [
    {
      "name": "camptocamp/systemd",
      "version_requirement": ">= 2.10.0 < 3.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 6.5.0 <7.0.0"
    }
  ],
  "source": "https://github.com/voxpupuli/puppet-dbbackup.git",
  "project_page": "https://github.com/voxpupuli/puppet-dbbackup",
  "issues_url": "https://github.com/voxpupuli/puppet-dbbackup/issues",
  "license": "AGPL-3.0",
  "tags": [
    "backup",
    "mysql",
    "database",
    "postgres",
    "postgresql",
    "pgsql",
    "ldap",
    "slapcat"
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 5.12.0 < 6.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "Archlinux"
    }
  ]
}
